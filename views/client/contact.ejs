<section id="contentSection">
    <div class="row">
      <div class="col-lg-8 col-md-8 col-sm-8">
        <div class="left_content">
          <div class="contact_area">
            <h2>Contact Us</h2>
            <p>Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labolore magna aliqua. Ut enim ad minim veniam. Lorem ipsum dosectetur adipisicing elit, sed do.Lorem ipsum dolor sit amet, consectetur adipisicing elit.</p>
            <form action="" method="post" class="contact_form">
              <input class="form-control" type="text" placeholder="Name*" name="ct_name">
              <input class="form-control" type="email" placeholder="Email*" name="ct_email">
              <textarea class="form-control" cols="30" rows="10" placeholder="Message*" name="ct_message"></textarea>
              <input type="submit" value="Send Message" onclick="return confirm('Thêm thành công')">
            </form>
          </div>
        </div>
      </div>
      <div class="col-lg-4 col-md-4 col-sm-4">
        <aside class="right_content">
          <div class="single_sidebar">
            <h2><span>Popular Post</span></h2>
            <ul class="spost_nav">
              <%data.lst_DN.forEach((item)=>{%>
              <li>
                <div class="media wow fadeInDown"> <a href="detail?id=<%=item['IDBaiViet']%>" class="media-left"> <img alt="" src="../../public/<%=item['Anh']%>"> </a>
                  <div class="media-body"> <a href="detail?id=<%=item['IDBaiViet']%>" class="catg_title"><%=item['TieuDe']%></a> </div>
                </div>
              </li>
              <%})%>
            </ul>
          </div>
        </aside>
      </div>
    </div>
  </section>